alacritty --hold -e sudo openvpn --auth-nocache --config $HOME/ProtonVPN\ Config/us-free-123059.protonvpn.tcp.ovpn --auth-user-pass $HOME/ProtonVPN\ Config/userlogin.txt &

#########
## Bar ##
#########
killall polybar && polybar &
#anytype &
#killall blueman-applet && blueman-applet &
greenclip daemon &
killall optimus-manager-qt && optimus-manager-qt &
