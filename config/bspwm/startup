#!/bin/sh

############
## System ##
############
killall sxhkd && sxhkd &
killall picom && picom &
killall dusnt && dunst &
killall numlockx && numlockx &
killall /usr/lib/xfce-polkit/xfce-polkit && /usr/lib/xfce-polkit/xfce-polkit &
killall $HOME/.config/dunst/Low-battery-notify && $HOME/.config/dunst/Low-battery-notify &
